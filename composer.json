{
    "name": "devgroup/yii2-grid-utils",
    "description": "Allows searching ActiveRecords with various conditions, events and addons",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Alexander Kozhevnikov",
            "email": "b37hr3z3n@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.0"
    },
    "require-dev": {
        "codeception/base": "^2.2",
        "codeception/verify": "^0.3.2",
        "codeception/specify": "^0.4.5"
    },
    "autoload": {
        "psr-4": {
            "DevGroup\\GridUtils\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DevGroup\\GridUtils\\Tests\\": "tests/"
        }
    },
    "extra": {
        "bootstrap": "DevGroup\\GridUtils\\GridModule"
    }
}
